@model Heksa.Models.Agen

@{
    ViewData["Title"] = "Delete Confirmation";
    Layout = "~/Views/Shared/Heksa_Layout.cshtml";
}

<h2>Delete Confirmation</h2>

<div>
    <h4>Are you sure you want to delete this Agen?</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            ID
        </dt>
        <dd class="col-sm-10">
            @Model.Id
        </dd>
    </dl>

    <form asp-action="DeleteConfirmed" asp-controller="Agen" method="post">
        <input type="hidden" asp-for="Id" />
        <div class="form-group">
            <input type="submit" value="Delete" class="btn btn-danger" />
            <a asp-action="Index" asp-controller="Agen" class="btn btn-secondary">Cancel</a>
        </div>
    </form>
</div>
