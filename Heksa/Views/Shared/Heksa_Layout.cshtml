<!DOCTYPE html>
<html lang="en" class="light-style layout-menu-fixed" dir="ltr" data-theme="theme-default" data-assets-path="../assets/" data-template="vertical-menu-template-free">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no, minimum-scale=1.0, maximum-scale=1.0" />
    <title> Heksa Insurance</title>
    <meta name="description" content="" />
    <link rel="icon" href="~/images/ss.png" type="image/png" />
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link href="https://fonts.googleapis.com/css2?family=Public+Sans:ital,wght@0,300;0,400;0,500;0,600;0,700;1,300;1,400;1,500;1,600;1,700&display=swap" rel="stylesheet" />
    <script src="../assets/vendor/js/helpers.js"></script>
    <script src="../assets/js/config.js"></script>
    <link rel="stylesheet" href="~/assets/vendor/fonts/boxicons.css" />
    <link rel="stylesheet" href="~/assets/vendor/css/core.css" class="template-customizer-core-css" />
    <link rel="stylesheet" href="~/assets/vendor/css/theme-default.css" class="template-customizer-theme-css" />
    <link rel="stylesheet" href="~/assets/css/demo.css" />
    <link rel="stylesheet" href="~/assets/vendor/libs/perfect-scrollbar/perfect-scrollbar.css" />
    <link rel="stylesheet" href="~/assets/vendor/libs/apex-charts/apex-charts.css" />
</head>

<body>
    <div class="layout-wrapper layout-content-navbar">
        <div class="layout-container">
            <aside id="layout-menu" class="layout-menu menu-vertical menu bg-menu-theme">
                <div class="app-brand demo">
                    <a class="menu-link" asp-area="" asp-controller="Home" asp-action="Index">
                        <span class="app-brand-logo demo">
                            <span class="app-brand-logo demo">
                                <svg width="25" viewBox="0 0 25 42" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                                    <img src="/Images/s1.png" width="100 height="150">
                                </svg>
                            </span>
                        </span>
                    </a>
                    <a href="javascript:void(0);" class="layout-menu-toggle menu-link text-large ms-auto d-block d-xl-none">
                        <i class="bx bx-chevron-left bx-sm align-middle"></i>
                    </a>
                </div>

                <div class="menu-inner-shadow"></div>

                <ul class="menu-inner py-1">
                    <li class="menu-header small text-uppercase"><span class="menu-header-text"></span></li>

                    <!-- Pension -->
                    <li class="menu-item">
                        <a href="javascript:void(0);" class="menu-link menu-toggle">
                            <i class="menu-icon tf-icons bx bx-layout"></i>
                            <div data-i18n="Pension">Menu</div>
                        </a>

                        <ul class="menu-sub">
                      
                            <li class="menu-item">
                                <a class="menu-link" asp-area="" asp-controller="DuaDanTiga" asp-action="SoalTiga">
                                    <div data-i18n="Client">Soal 3</div>
                                </a>
                            </li>

                            <li class="menu-item">
                                <a class="menu-link" asp-area="" asp-controller="DuaDanTiga" asp-action="PasswordValidation">
                                    <div data-i18n="Client">Soal 2</div>
                                </a>
                            </li>
                        </ul>
                    </li>
                    <li class="menu-item">
                        <a class="menu-link" asp-area="" asp-controller="Agen" asp-action="AgenList">
                            <i class="menu-icon tf-icons bx bx-layout"></i>
                            <div data-i18n="Company">Agen List</div>
                        </a>

                    <li class="menu-header small text-uppercase"><span class="menu-header-text"></span></li>
     
                    <li class="menu-item">
                        <a class="menu-link" asp-controller="Education" asp-action="Educations">
                            <i class="menu-icon tf-icons bx bx-user"></i>
                            <div data-i18n="">Education List</div>
                        </a>
                    </li>
                    <li class="menu-item">
                        <a class="menu-link" asp-controller="Attachment" asp-action="AttachmentsList">
                            <i class="menu-icon tf-icons bx bx-user"></i>
                            <div data-i18n="">Attachment List</div>
                        </a>
                    </li>

                                   <li class="menu-item">
                    <a class="menu-link" asp-controller="WorkExperience" asp-action="WorkExperiences">
                    <i class="menu-icon tf-icons bx bx-user"></i>
                    <div data-i18n="">Work Experience List</div>
                    </a>
                    </li>


                    <li class="menu-header small text-uppercase">
                        <span class="menu-header-text">

                        </span>
                    </li>
                </ul>
            </aside>

            <div class="layout-page">
                <!-- Navbar -->
                <!-- / Navbar -->
                <!-- Content wrapper -->
                <div class="content-wrapper">
                    <!-- Content -->

                    <div class="container-xxl flex-grow-1 container-p-y">
                        <div class="row">
                            @RenderBody()
                        </div>

                    </div>

                    <div class="content-backdrop fade"></div>
                </div>
                <div class="content-wrapper">

                    <!-- Content -->

                    <div class="container-xxl flex-grow-1 container-p-y">


                        @RenderBody()




                    </div>
                    <!-- Footer -->
                    <footer class="content-footer footer bg-footer-theme">
                        <div class="container-xxl d-flex flex-wrap justify-content-between py-2 flex-md-row flex-column">
                            <div class="mb-2 mb-md-0">
                                <span id="currentYear"></span>
                            </div>
                            <div class="footer-right">
                                by <a href="https://www.heksainsurance.co.id/" target="_blank" class="footer-link fw-bolder">Heksa Insurance</a>
                            </div>
                        </div>
                        <div class="container-xxl d-flex justify-content-end">
                            <div id="currentTime"></div>
                        </div>
                    </footer>

                    <script>
                        const currentYearElement = document.getElementById('currentYear');
                        const currentTimeElement = document.getElementById('currentTime');

                        function updateTime() {
                            const currentDate = new Date();
                            const currentYear = currentDate.getFullYear();
                            const day = currentDate.toLocaleDateString('en-US', { weekday: 'long' });
                            const date = currentDate.toLocaleDateString('en-US');
                            const timeOptions = { hour: '2-digit', minute: '2-digit', second: '2-digit', hour12: false };
                            const currentTime = currentDate.toLocaleTimeString('en-US', timeOptions);

                            currentYearElement.innerText = currentYear;
                            currentTimeElement.innerText = `${day}, ${date} | ${currentTime}`;
                        }

                        updateTime(); //
                        setInterval(updateTime, 1000); //
                    </script>






                    <!-- / Footer -->


                    <div class="content-backdrop fade"></div>
                </div>
            </div>


        </div>
    </div>


    <!-- Vendor Scripts -->
    <script src="../assets/vendor/libs/jquery/jquery.js"></script>
    <script src="../assets/vendor/libs/popper/popper.js"></script>
    <script src="../assets/vendor/js/bootstrap.js"></script>
    <script src="../assets/vendor/libs/perfect-scrollbar/perfect-scrollbar.js"></script>
    <script src="../assets/vendor/js/menu.js"></script>
    <script src="../assets/vendor/libs/apex-charts/apexcharts.js"></script>
    <script src="../assets/js/main.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
    <script src="../assets/js/dashboards-analytics.js"></script>
    <script async defer src="https://buttons.github.io/buttons.js"></script>

    @RenderSection("Scripts", required: false)
</body>

</html>
